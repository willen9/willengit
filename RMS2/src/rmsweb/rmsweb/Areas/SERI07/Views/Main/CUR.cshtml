                                var res = true;
                                if (value != "") {
                                    $.ajax({
                                        url: '@Url.Action("SerialNo", "Main")',
                                        data: JSON.stringify({ "SerialNo": value }),
                                        type: 'POST',
                                        async: false,
                                        contentType: 'application/json; charset=utf-8',
                                        success: function (data) {
                                            if (data == false) {
                                                res = false;
                                            }
                                        }
                                    })
                                }
                                return res;
                            }
                        }
                    }
                }
            }
        });

