    function updateProductDWarrantyEDate() {
        if ($("#id_Warranty_DWarrantyPeriod").val() == "") {
            return
        }
        if ($("#id_Warranty_DWarrantySDate").val() == "") {
            //$("#id_WarrantyPeriod").val("0")
            return;
        }
        var Edate = new Date($("#id_Warranty_DWarrantySDate").val());
        Edate.setMonth(Edate.getMonth() + parseInt($("#id_Warranty_DWarrantyPeriod").val()));
        var year = Edate.getFullYear();//当前年份
        var month = Edate.getMonth() + 1;//当前月份
        var day = Edate.getDate();//当前日
        if (month.toString().length == 1) {
            month = "0" + month;
        }
        if (day.toString().length == 1) {
            day = "0" + day;
        }
        $("#id_Warranty_DWarrantyEDate").val(year + "/" + month + "/" + day);

    };

