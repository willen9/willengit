                                            webGrid.Column(columnName: "", header: "動作", style: "actioncolumn",
                                                format:
                                                @<text>
                                                    @if (@ViewBag.Type == "Details")
                                                    {
                                                        <a class='btn btn-link' data-dismiss='modal'>編輯</a>
                                                        <a class='btn btn-link' data-dismiss='modal'>刪除</a>
                                                    }
                                                    else
                                                    {
                                                        <a class='btn btn-link' data-dismiss='modal' onclick='ProcessDExit(this)'>編輯</a>
                                                        <a class='btn btn-link' data-dismiss='modal' onclick='RemoveProcessD(this)'>刪除</a>
                                                    }
                                                </text>),
                                                webGrid.Column(columnName: "ItemId", header: "項次", style: "ItemId",
                                                format:
                                                @<text>
                                                    @item.ItemId
                                                    <input type="hidden" class="strPItemId" name="strPItemId" value='@item.ItemId' tabindex="37">
                                                </text>),
                                                webGrid.Column(columnName: "ProcessDate", header: "處理日期", style: "ProcessDate",
                                                format:
                                                @<text>
                                                    @item.ProcessDate
                                                    <input type="hidden" class="strPProcessDate" name="strPProcessDate" value='@item.ProcessDate' tabindex="38">
                                                </text>),
                                                webGrid.Column(columnName: "ProcessExplanation", header: "處理描述", style: "ProcessExplanation",
                                                format:
                                                @<text>
                                                    @item.ProcessExplanation
                                                    <input type="hidden" class="strPProcessExplanation" name="strPProcessExplanation" value='@item.ProcessExplanation' tabindex="39">
                                                </text>),
                                                webGrid.Column(columnName: "ProcessMan", header: "處理人員", style: "ProcessMan",
                                                format:
                                                @<text>
                                                    @item.ProcessMan
                                                    <input type="hidden" class="strPProcessMan" name="strPProcessMan" value='@item.ProcessMan' tabindex="40">
                                                </text>),
                                                webGrid.Column(columnName: "Remark", header: "備註", style: "Remark",
                                                format:
                                                @<text>
                                                    @item.Remark
                                                    <input type="hidden" class="strPRemark" name="strPRemark" value='@item.Remark' tabindex="41">
                                                </text>)
)
)
                    </div>
                </div>

                <p style="color: red;">@ViewBag.Msg</p>
                <div class="text-right">
                    @if (@ViewBag.Type == "Edit")
                    {
                        <button type="button" class="btn btn-primary" name="action" value="Edit" onclick="btnExit()" tabindex="23">存檔</button>
                        <button type="button" class="btn btn-primary" name="action" value="EditAgain" onclick="btnExitAgain()" tabindex="24">存檔並新增</button>
                        <a class="btn btn-primary" href="@Url.Action("Index", "Main", null)">取消</a>
                    }
                    else if (@ViewBag.Type == "Details")
                    {
                        <a class="btn btn-primary" href="@Url.Action("Edit", "Main", new { ProductNo = @ViewBag.ProductNo, SerialNo = @ViewBag.SerialNo, CustomerId = @ViewBag.CustomerId, SaleDate = @ViewBag.SaleDate })">編輯</a>
                        <a class="btn btn-primary">保固設定</a>
                        <a class="btn btn-primary" onclick="deleteInfo('@Url.Action("Delete", "Main", new { ProductNo = @ViewBag.ProductNo, SerialNo = @ViewBag.SerialNo, CustomerId = @ViewBag.CustomerId, SaleDate = @ViewBag.SaleDate })', '@ViewBag.ContractNo')">刪除</a>
                        <a class="btn btn-primary" href="@Url.Action("Index", "Main", null)">返回列表</a>
                    }
                    else
                    {
                        <button type="button" class="btn btn-primary" name="action" value="Save" onclick="btnAdd()" tabindex="25">存檔</button>
                        <button type=button class="btn btn-primary" name="action" value="SaveAgain" onclick="btnAddAgain()" tabindex="26">存檔並新增</button>
                        <a class="btn btn-primary" href="@Url.Action("Index", "Main", null)">取消</a>
                    }
                </div>
            </form>
        </div>
    </div>
