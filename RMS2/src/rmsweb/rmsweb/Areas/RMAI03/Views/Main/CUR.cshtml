            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                $("#CustomerId").val(data.CustomerId);     //品名
                $("#CustomerName").html(data.CustomerName);
                $("#CustomerType").val(data.CustomerType);        //規格
                $("#ProductNo").val(data.ProductNo);        //品號
