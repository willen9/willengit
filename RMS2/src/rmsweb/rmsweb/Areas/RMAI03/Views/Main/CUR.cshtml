                $("#SerialNo").val(data.SerialNo);     //單位
                $("#WarrantyStartDate").val(data.WarrantyStartDate);        //規格
                $("#WarrantyExpiryDate").val(data.WarrantyExpiryDate);     //單位
                if ($("#WarrantyStartDate").val() < $("#id_OrderDate").val()
                && $("#id_OrderDate").val() < $("#WarrantyExpiryDate").val())        //續約
                {
                    document.getElementById('UnderWarranty').checked = true;
