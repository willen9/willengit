                        $("#VendorWarrantyStartDate").val(data.WarrantySDate);     //品名
                        $("#VendorWarrantyExpiryDate").val(data.WarrantyEDate);     //品名
                    }
                })
            },
            error: function () {
                ajaxTrue = false;
                $("#msg_information").html("資料不存在！");
                $('.msg-modal-lg').modal();
                $("#CustomerId").val("");     //品名
                $("#CustomerName").html("");     //品名
                $("#CustomerType").val("");     //品名
                $("#ProductNo").val("");     //品名
                $("#ProductName").val("");     //品名
                $("#SerialNo").val("");     //品名
                $("#WarrantyStartDate").val("");     //品名
                $("#WarrantyExpiryDate").val("");     //品名
                document.getElementById('UnderWarranty').checked = false;
                document.getElementById('Returned').checked = false;
                $("#VendorWarrantyStartDate").val("");     //品名
                $("#VendorWarrantyExpiryDate").val("");     //品名
                return;
            }
        })
    };

