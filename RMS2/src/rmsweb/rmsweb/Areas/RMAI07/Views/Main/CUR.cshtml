                $.ajax({
                    url: '@Url.Action("GetOrderTypeName", "Main", new { Area = "CONI01" })',
                    data: JSON.stringify({ "OrderType": $("#RFQType").val().trim(), "OrderCategory": "C4" }),
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {
                        $('#ordersname').text(data.OrderSName);
                        $('#AutoConfirm').val(data.AutoConfirm);
                        $('#CheckExOrder').val(data.CheckExOrder);
                        $.ajax({
                            url: '@Url.Action("GetOrderTypeNo", "Main",new { Area = "SUPI05"})',
                            data: JSON.stringify({ "OrderType": $("#RFQType").val().trim() }),
                            type: 'POST',
                            contentType: 'application/json; charset=utf-8',
                            success: function (data) {
                                var strData = data.split('-');
                                if (strData[0] == "YES") {
                                    $('#RFQNo').val(strData[1]);
                                    if (strData[1] == "") {
                                        document.getElementById("RFQNo").readOnly = "";
                                    } else {
                                        document.getElementById("RFQNo").readOnly = "readonly";
                                    }
                                } else if (strData[0] == "NO") {
                                    $('#RFQNo').val(strData[1]);
                                    document.getElementById("RFQNo").readOnly = "";
                                } else {
                                    $("#msg_information").html(strData[1]);
                                    document.getElementById("RFQNo").readOnly = "";
                                    $('.msg-modal-lg').modal();
                                }
                            }
                        })
                    },
                    error: function () {
                        $("#msg_information").html("單別不存在！");
                        $('.msg-modal-lg').modal();
                        $("#RFQType").val("")
                        $('#ordersname').text("");
                        $('#RFQNo').val("");
                        document.getElementById("RFQNo").readOnly = "";
                        $('#AutoConfirm').val("");
                        $('#CheckExOrder').val("");
                        return;
                    }
                })
