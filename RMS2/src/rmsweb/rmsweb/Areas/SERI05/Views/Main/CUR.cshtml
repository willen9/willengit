        $("#id_PhoneService_DProcessManName").html($(id).closest('tr').find(".EmployeeNameTyep").html());
        $('.PhoneService_D-modal').modal();
    }

    function SelectOrderType(id) {
        $("#id_PhoneSerType").val($(id).closest('tr').find(".OrderTypeType").html());
        $("#ordersname").html($(id).closest('tr').find(".OrderSNameType").html());
        $.ajax({
            url: '@Url.Action("GetOrderTypeNo", "Main", new { Area = "SUPI05" })',
            data: JSON.stringify({ "OrderType": $("#id_PhoneSerType").val().trim() }),
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                var strData = data.split('-');
                if (strData[0] == "YES") {
                    $('#id_PhoneSerNo').val(strData[1]);
                    if (strData[1] == "") {
                        document.getElementById("id_PhoneSerNo").readOnly = "";
                    } else {
                        document.getElementById("id_PhoneSerNo").readOnly = "readonly";
                    }
                } else if (strData[0] == "NO") {
                    $('#id_PhoneSerNo').val(strData[1]);
                    document.getElementById("id_PhoneSerNo").readOnly = "";
                } else {
                    $("#msg_information").html(strData[1]);
                    document.getElementById("id_PhoneSerNo").readOnly = "";
                    $('.msg-modal-lg').modal();
                }
            }
        })
    };
</script>
