<div class='modal fade closed-detail-modal' tabindex='-1' role='dialog' aria-hidden='true'>
    <div class='modal-dialog'>
        <div class='modal-content'>
            <div class='modal-header'>
                <h4 class='modal-title'>序號明細</h4>
            </div>
            <div class='modal-body'>
                <span style="display:inline;visibility:hidden;" id="id_OrderType"></span>
                <span style="display:inline;visibility:hidden;" id="id_OrderNo" ></span>
                <span style="display:inline;visibility:hidden;" id="id_ItemId" ></span>

                <label for="id_customertype" class="control-label col-sm-12">品號：<span style="display:inline;" id="id_ProductNo"></span></label>
                
                
                <label for="id_customertype" class="control-label col-sm-12">品名：<span style="display:inline;" id="id_ProductName"></span></label>
                
                <br/>
                <label for="id_customertype" class="control-label col-sm-12">數量：<span style="display:inline;" id="id_QTY"></span></label>
                
                <label for="id_customertype" class="control-label col-sm-12">序號數量：<span style="display:inline;" id="id_SerialQTY"></span></label>
                
                <hr/>
                <br/>
                <button type='button' class='btn btn-default' data-dismiss='modal'>關閉</button>
                <div id="divProductSerialGrid" style="height: 500px;overflow-y: auto;">
                    <table class="table table-striped table-hover table-condensed" data-swhgajax="true" data-swhgcontainer="divProductSerialGrid" data-swhgcallback="">
                        <thead>
                            <tr>
                                <th scope="col">
                                    動作
                                </th>
                                <th scope="col">
                                    項次
                                </th>
                                <th scope="col">
                                    序號
                                </th>
                                <th scope="col">
                                    狀態
                                </th>
                                @*<th scope="col">
                                    指定結案
                                </th>*@
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <button type='button' class='btn btn-default' onclick="SaveSerial()"> 保存</button>
                </div>
            <div class='modal-footer'>
            </div>
        </div>
    </div>
</div>